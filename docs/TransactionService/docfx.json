{
  "metadata": [
    {
      "src": [
        {
          "files": ["TransactionService.csproj"],
          "src": "../../Microservices/TransactionService"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["*.md", "toc.yml"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "TransactionService",
      "_appTitle": "TransactionService",
      "_enableSearch": true,
      "pdf": true
    }
  }
}